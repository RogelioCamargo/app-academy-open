<h1><%= @cat.name %>'s Profile</h1>

<table>
	<thead>
		<tr>
			<th>Birth Date</th>
			<th>Color</th>
			<th>Sex</th>
		</tr>
	</thead>
	<tbody>
		<tr>
			<td><%= @cat.birth_date %></td>
			<td><%= @cat.color %></td>
			<td><%= @cat.sex %></td>
		</tr>
	</tbody>
</table>

<%= link_to 'Edit details', edit_cat_url(@cat) %>

